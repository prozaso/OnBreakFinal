            <materialDesign:PopupBox x:Name="popGestionContrato" Grid.Row="17" Style="{StaticResource MaterialDesignMultiFloatingActionPopupBox}" PlacementMode="RightAndAlignMiddles" UnfurlOrientation="Horizontal"
                            ToolTip="Manage items" HorizontalAlignment="Stretch" Grid.Column="10" Height="Auto" Width="Auto" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" FontWeight="Bold" FontFamily="Arial" FontSize="14">
                <StackPanel Orientation="Horizontal">

                    <Button x:Name="popCrearContrato" ToolTip="Crear Contrato" Click="PopCrearContrato_Click" >
                        <materialDesign:PackIcon Kind="AccessPoint" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Height="Auto" Width="Auto" Margin="4" />
                    </Button>

                    <Button x:Name="popActualizarContrato" ToolTip="Actualizar Contrato" Click="PopActualizarContrato_Click" >
                        <materialDesign:PackIcon Kind="Update" Margin="4" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Height="Auto" Width="Auto" />
                    </Button>

                    <Button x:Name="popTerminarContrato" ToolTip="Terminar Contrato" Click="PopTerminarContrato_Click" >
                        <materialDesign:PackIcon Kind="CloseBox" Margin="4" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Height="Auto" Width="Auto" />
                    </Button>

                </StackPanel>
            </materialDesign:PopupBox>
