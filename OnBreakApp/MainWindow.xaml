<Window x:Class="OnBreakApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OnBreakApp"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:materialDesignConverters="clr-namespace:MaterialDesignThemes.Wpf.Converters;assembly=MaterialDesignThemes.Wpf"
        xmlns:smtx="clr-namespace:ShowMeTheXAML;assembly=ShowMeTheXAML"
        mc:Ignorable="d"
        FontFamily="Arial"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        Background="{DynamicResource MaterialDesignPaper}"
        WindowStartupLocation="CenterScreen" WindowStyle="None" ResizeMode="NoResize" SizeToContent="WidthAndHeight" Icon="img/dark_logo_transparent_background.png" FontSize="14" Width="auto" Height="auto" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Grid x:Name="barraTitulo" Background="#FFFF2424" Height="30" VerticalAlignment="Top" MouseDown="BarraTitulo_MouseDown">

            <StackPanel Orientation="Horizontal" HorizontalAlignment="Left">
                <Label Content="Contraste" FontWeight="Bold" FontStyle="Italic" Margin="5,0,0,0" Padding="5,7,5,0"/>
                <ToggleButton Style="{StaticResource MaterialDesignSwitchDarkToggleButton}" ToolTip="MaterialDesignSwitchDarkToggleButton" IsChecked="True" Padding="0" Margin="0,0,5,0" Width="50" FontFamily="Arial" FontSize="14" Checked="ToggleButton_Checked" Unchecked="ToggleButton_Unchecked"/>

                <Button x:Name="btnGestionCliente" Style="{StaticResource MaterialDesignRaisedButton}" Width="140" ToolTip="Resource name: MaterialDesignRaisedAccentButton" materialDesign:RippleAssist.Feedback="#DD000000" Height="25" Padding="0"  FontWeight="Bold" FontFamily="Arial" FontStyle="Italic" Click="BtnGestionCliente_Click">
                    Gestion Clientes
                </Button>

                <Button x:Name="btnListaClientes" Style="{StaticResource MaterialDesignRaisedButton}" Width="140" ToolTip="Resource name: MaterialDesignRaisedAccentButton" materialDesign:RippleAssist.Feedback="#DD000000" Height="25" Padding="0" FontWeight="Bold" FontFamily="Arial"  FontStyle="Italic" Click="BtnListaClientes_Click">
                    Lista Clientes
                </Button>

                <Button x:Name="btnGestionContrato" Style="{StaticResource MaterialDesignRaisedButton}" Width="140" ToolTip="Resource name: MaterialDesignRaisedAccentButton" materialDesign:RippleAssist.Feedback="#DD000000" Height="25" Padding="0"  FontWeight="Bold" FontFamily="Arial"  FontStyle="Italic" Click="BtnGestionContrato_Click" >
                    Gestion Contrato
                </Button>

                <Button x:Name="btnListaContratos" Style="{StaticResource MaterialDesignRaisedButton}" Width="140" ToolTip="Resource name: MaterialDesignRaisedAccentButton" materialDesign:RippleAssist.Feedback="#DD000000" Height="25" Padding="0"  FontWeight="Bold" FontFamily="Arial" FontStyle="Italic" Click="BtnListaContratos_Click" >
                    Lista Contratos
                </Button>

            </StackPanel>

            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">



                <Button x:Name="btnMinVentanaMain" Style="{StaticResource MaterialDesignRaisedButton}" Width="50" materialDesign:ButtonProgressAssist.Value="-1" materialDesign:ButtonProgressAssist.IsIndicatorVisible="True" materialDesign:ButtonProgressAssist.IsIndeterminate="True" Margin="2,0" Click="BtnMinVentanaMain_Click">
                    <materialDesign:PackIcon Kind="WindowMinimize" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
                </Button>

                <Button x:Name="btnCerrarVentanaMain" Style="{StaticResource MaterialDesignRaisedButton}" Width="50" materialDesign:ButtonProgressAssist.Value="-1" materialDesign:ButtonProgressAssist.IsIndicatorVisible="True" materialDesign:ButtonProgressAssist.IsIndeterminate="True" Margin="2,0" Click="BtnCerrarVentanaMain_Click">
                    <materialDesign:PackIcon Kind="WindowClose" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
                </Button>

            </StackPanel>

        </Grid>
        <Grid x:Name="barraCache" Height="30" VerticalAlignment="Top" Margin="0,30,0,0">
            <StackPanel Orientation="Horizontal" VerticalAlignment="Top" Height="30" >
                <TextBlock Text="Fecha ultimo respaldo: " Margin="5,2,0,2"/>
                <TextBlock x:Name="txtfechaCache" Margin="5,2,0,2" />
            </StackPanel>

            <Button x:Name="btnRespaldo" Style="{StaticResource MaterialDesignRaisedButton}" Content="Respaldar" Height="25" Width="98" HorizontalAlignment="Right" VerticalAlignment="Center" Padding="0" HorizontalContentAlignment="Center" Margin="0,3,5,0" Click="btnRespaldo_Click"/>

        </Grid>


        <Grid x:Name="gridGestionCliente" MinHeight="520" MaxHeight="520" MinWidth="840" MaxWidth="840" Width="840" Height="520" Margin="0,60,0,0">

            <Grid.RowDefinitions>
                <RowDefinition Height="45"/>
                <RowDefinition Height="45"/>
                <RowDefinition Height="9"/>
                <RowDefinition Height="45"/>
                <RowDefinition Height="9"/>
                <RowDefinition Height="45"/>
                <RowDefinition Height="9"/>
                <RowDefinition Height="45"/>
                <RowDefinition Height="9"/>
                <RowDefinition Height="45"/>
                <RowDefinition Height="9"/>
                <RowDefinition Height="45"/>
                <RowDefinition Height="9"/>
                <RowDefinition Height="45"/>
                <RowDefinition Height="9"/>
                <RowDefinition Height="45"/>
                <RowDefinition Height="45"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="101*"/>
                <ColumnDefinition Width="163*"/>
                <ColumnDefinition Width="9"/>
                <ColumnDefinition Width="294*"/>
                <ColumnDefinition Width="9"/>
                <ColumnDefinition Width="157*"/>
                <ColumnDefinition Width="104*"/>
            </Grid.ColumnDefinitions>

            <Button x:Name="btnBuscarCliente" Style="{StaticResource MaterialDesignRaisedButton}" FontFamily="Arial" FontWeight="Bold"  Height="Auto" Width="auto" Grid.Row="1" Padding="0" Grid.Column="1" Click="BtnBuscarCliente_Click">
                <StackPanel>
                    <TextBlock Text="RUT" HorizontalAlignment="Center"/>
                    <TextBlock Text="(Buscar)" HorizontalAlignment="Center" FontSize="10"/>
                </StackPanel>
            </Button>

            <TextBox x:Name="txtRut" materialDesign:HintAssist.Hint="Ej: 12.345.678-9" Grid.Row="1" Grid.Column="3" VerticalContentAlignment="Bottom" HorizontalContentAlignment="Left" Padding="0" FontFamily="Arial" FontSize="14" />
            <TextBox x:Name="txtRazon" materialDesign:HintAssist.Hint="Razon Social" Grid.Row="3" Grid.Column="3" VerticalContentAlignment="Bottom" HorizontalContentAlignment="Left" Padding="0" FontFamily="Arial" FontSize="14" />
            <TextBox x:Name="txtNombre" materialDesign:HintAssist.Hint="Nombre" Grid.Row="5" Grid.Column="3" Height="auto" VerticalContentAlignment="Bottom" HorizontalContentAlignment="Left" Padding="0" FontFamily="Arial" FontSize="14" />
            <TextBox x:Name="txtMail" materialDesign:HintAssist.Hint="Ej: esteEs@miEmail.cl" Grid.Row="7" Grid.Column="3" Height="auto" VerticalContentAlignment="Bottom" HorizontalContentAlignment="Left" Padding="0" FontFamily="Arial" FontSize="14" />
            <TextBox x:Name="txtDireccion" materialDesign:HintAssist.Hint="Direccion" Grid.Row="15" Grid.Column="3" Height="auto" VerticalContentAlignment="Bottom" HorizontalContentAlignment="Left" Padding="0" FontFamily="Arial" FontSize="14"  Grid.ColumnSpan="3" />
            <TextBox x:Name="txtTelefono" materialDesign:HintAssist.Hint="Telefono" Grid.Row="9" Grid.Column="3" Height="auto" VerticalContentAlignment="Bottom" HorizontalContentAlignment="Left" Padding="0" FontFamily="Arial" FontSize="14" />
            <Button x:Name="btnGuardarCliente" Style="{StaticResource MaterialDesignRaisedButton}" Content="Guardar" Grid.Row="1" Grid.Column="5" Height="Auto" Padding="0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontFamily="Arial" FontWeight="Bold" Click="BtnGuardarCliente_Click" />
            <Button x:Name="btnActualizarCliente" Style="{StaticResource MaterialDesignRaisedButton}" Content="Actualizar" Grid.Row="3" Grid.Column="5" Height="Auto" Padding="0" FontFamily="Arial" FontWeight="Bold" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Click="BtnActualizarCliente_Click" />

            <Button x:Name="btnListadoClientes" Style="{StaticResource MaterialDesignRaisedAccentButton}" Grid.Row="7" Grid.Column="5" Height="Auto" Grid.RowSpan="3" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Center" Padding="0" FontFamily="Arial" FontWeight="Bold" FontSize="16" Click="BtnListadoClientes_Click">
                <StackPanel>
                    <TextBlock Text="Lista de" TextAlignment="Center" Margin="0"/>
                    <TextBlock Text="Clientes" TextAlignment="Center"/>
                </StackPanel>
            </Button>

            <Button x:Name="btnLimpiarCliente" Style="{StaticResource MaterialDesignRaisedButton}" Grid.Row="11" Grid.Column="5" Height="Auto" FontFamily="Arial" FontWeight="Bold" Grid.RowSpan="3" Padding="0" Click="BtnLimpiarCliente_Click" >
                <StackPanel>
                    <TextBlock Text="Limpiar" TextAlignment="Center"/>
                    <TextBlock Text="Campos" TextAlignment="Center"/>
                </StackPanel>
            </Button>

            <ComboBox x:Name="cboActividad" materialDesign:HintAssist.Hint="Seleccionar" Style="{StaticResource MaterialDesignFloatingHintComboBox}" Grid.Row="11" Grid.Column="3" VerticalAlignment="Stretch" Padding="0" HorizontalContentAlignment="Left" SelectedValuePath="IdActividadEmpresa" DisplayMemberPath="Descripcion" FontFamily="Arial" FontSize="14" />
            <ComboBox x:Name="cboTipo" materialDesign:HintAssist.Hint="Seleccionar" Style="{StaticResource MaterialDesignFloatingHintComboBox}" Grid.Row="13" Grid.Column="3" VerticalAlignment="Stretch" Padding="0" HorizontalContentAlignment="Left" SelectedValuePath="IdTipoEmpresa" DisplayMemberPath="Descripcion" FontFamily="Arial" FontSize="14" />
            <Label Content="Razón Social" Grid.Row="3" VerticalContentAlignment="Center" FontFamily="Arial" FontSize="14" FontWeight="Bold" Grid.Column="1"/>
            <Label Content="Nombre" Grid.Row="5" VerticalContentAlignment="Center" FontFamily="Arial" FontSize="14" FontWeight="Bold" Grid.Column="1"/>
            <Label Content="Mail" Grid.Row="7" VerticalContentAlignment="Center" FontFamily="Arial" FontSize="14" FontWeight="Bold" Grid.Column="1"/>
            <Label Content="Direccion" Grid.Row="15" VerticalContentAlignment="Center" FontFamily="Arial" FontSize="14" FontWeight="Bold" Grid.Column="1"/>
            <Label Content="Telefono" Grid.Row="9" VerticalContentAlignment="Center" FontFamily="Arial" FontSize="14" FontWeight="Bold" Grid.Column="1"/>
            <Label Content="Actividad" Grid.Row="11" VerticalContentAlignment="Center" FontFamily="Arial" FontSize="14" FontWeight="Bold" Grid.Column="1"/>
            <Label Content="Tipo" Grid.Row="13" VerticalContentAlignment="Center" FontFamily="Arial" FontSize="14" FontWeight="Bold" Grid.Column="1"/>
            <Button x:Name="btnEliminarCliente" Style="{StaticResource MaterialDesignRaisedButton}" Content="Eliminar" Grid.Row="5" Grid.Column="5" Height="Auto" Padding="0" FontFamily="Arial" FontWeight="Bold" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Click="btnEliminarCliente_Click" />
            <Image x:Name="image" Source="img/white_logo_transparent_background.png" Margin="23,10,10,1" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.RowSpan="2" Height="80" Width="68"/>

        </Grid>


        <Grid x:Name="gridListaClientes" MinWidth="806" MaxWidth="806" VerticalAlignment="Bottom" MinHeight="411" MaxHeight="411" Width="806" Height="411" Margin="0,60,0,0" >

            <Grid.RowDefinitions>
                <RowDefinition Height="45"/>
                <RowDefinition Height="45"/>
                <RowDefinition Height="45"/>
                <RowDefinition Height="276"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="84*"/>
                <ColumnDefinition Width="162*"/>
                <ColumnDefinition Width="12*"/>
                <ColumnDefinition Width="166*"/>
                <ColumnDefinition Width="11*"/>
                <ColumnDefinition Width="182*"/>
                <ColumnDefinition Width="12*"/>
                <ColumnDefinition Width="156*"/>
                <ColumnDefinition Width="22*"/>
            </Grid.ColumnDefinitions>


            <DataGrid x:Name="dgClientes" Width="auto" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Grid.Row="3" Grid.ColumnSpan="9" IsReadOnly="True" AutoGenerateColumns="False" ItemsSource="{Binding Items3}" CanUserSortColumns="True" CanUserAddRows="False" materialDesign:DataGridAssist.CellPadding="4 2 2 2" materialDesign:DataGridAssist.ColumnHeaderPadding="4 2 2 2" FontFamily="Arial" FontSize="14" MaxHeight="228" Margin="20,24" MinWidth="766" MinHeight="228" MaxWidth="766" BorderBrush="#FFF44336">
                <DataGrid.Columns>
                    <DataGridTextColumn Binding="{Binding RutCliente}" ClipboardContentBinding="{x:Null}" Header="Rut"/>
                    <DataGridTextColumn Binding="{Binding RazonSocial}" ClipboardContentBinding="{x:Null}" Header="Razon Social"/>
                    <DataGridTextColumn Binding="{Binding NombreContacto}" ClipboardContentBinding="{x:Null}" Header="Contacto"/>
                    <DataGridTextColumn Binding="{Binding MailContacto}" ClipboardContentBinding="{x:Null}" Header="Mail"/>
                    <DataGridTextColumn Binding="{Binding Direccion}" ClipboardContentBinding="{x:Null}" Header="Direccion"/>
                    <DataGridTextColumn Binding="{Binding Telefono}" ClipboardContentBinding="{x:Null}" Header="Fono"/>
                    <DataGridTextColumn Binding="{Binding ActividadEmpresa.Descripcion}" ClipboardContentBinding="{x:Null}" Header="Actividad Empresa"/>
                    <DataGridTextColumn Binding="{Binding TipoEmpresa.Descripcion}" ClipboardContentBinding="{x:Null}" Header="Tipo Empresa"/>
                </DataGrid.Columns>
            </DataGrid>
            <TextBox x:Name="txtBuscarRutCliente" Grid.Column="3" Grid.Row="2" TextWrapping="Wrap" materialDesign:HintAssist.Hint="Ej: 12.345.678-9" HorizontalContentAlignment="Left" Padding="0" VerticalContentAlignment="Bottom" FontFamily="Arial" FontSize="14"/>
            <ComboBox x:Name="cboFiltrarTipoCliente" Grid.Column="7" Grid.Row="2" materialDesign:HintAssist.Hint="Seleccionar" Style="{StaticResource MaterialDesignFloatingHintComboBox}" VerticalAlignment="Stretch" DisplayMemberPath="Descripcion" SelectedValuePath="IdTipoEmpresa" HorizontalContentAlignment="Left" Padding="0" VerticalContentAlignment="Center" FontSize="14" FontFamily="Arial" />
            <Button x:Name="btnFiltrarActividadCliente" Style="{StaticResource MaterialDesignRaisedButton}" Content="Buscar por Actividad" Grid.Column="5" Height="Auto" Grid.Row="1"  FontFamily="Arial" FontWeight="Bold" Padding="0" Click="BtnFiltrarActividadCliente_Click"/>
            <Button x:Name="btnFiltrarRutCliente" Style="{StaticResource MaterialDesignRaisedButton}" Content="Buscar por RUT" Grid.Column="3" Height="Auto" Grid.Row="1"  FontFamily="Arial" FontWeight="Bold" Padding="0" Click="BtnFiltrarRutCliente_Click" />
            <Button x:Name="btnFiltrarTipoCliente" Style="{StaticResource MaterialDesignRaisedButton}" Content="Buscar por tipo" Grid.Column="7" Height="Auto" Grid.Row="1" FontFamily="Arial" FontWeight="Bold" Padding="0" Click="BtnFiltrarTipoCliente_Click"/>
            <ComboBox x:Name="cboFiltrarActividadCliente" Grid.Column="5" Grid.Row="2" Style="{StaticResource MaterialDesignFloatingHintComboBox}" materialDesign:HintAssist.Hint="Seleccionar" VerticalAlignment="Stretch" DisplayMemberPath="Descripcion" SelectedValuePath="IdActividadEmpresa" HorizontalContentAlignment="Left" Padding="0" VerticalContentAlignment="Center" FontFamily="Arial" FontSize="14" />
            <Button x:Name="btnClienteToGestionCliente" Style="{StaticResource MaterialDesignRaisedButton}" Content="Cliente a Gestion" Height="Auto" Grid.Row="1"  FontFamily="Arial" FontWeight="Bold" Padding="5"  Margin="0,10,10,10" Grid.Column="1" HorizontalContentAlignment="Left" Click="btnClienteToGestionCliente_Click" Visibility="Hidden"/>
            <Button x:Name="btnClienteToListaContratos" Style="{StaticResource MaterialDesignRaisedButton}" Content="Cliente a Contratos" Height="Auto" Grid.Row="2"  FontFamily="Arial" FontWeight="Bold" Padding="5" Margin="0,10,10,10" Grid.Column="1" HorizontalContentAlignment="Left" Click="btnClienteToListaContratos_Click" />
            <Image x:Name="image3" HorizontalAlignment="Left" Grid.RowSpan="2" VerticalAlignment="Top" Margin="10" Source="img/white_logo_transparent_background.png"/>

        </Grid>


        <Grid x:Name="gridGestionContrato" MinWidth="806" MinHeight="633" MaxWidth="806" MaxHeight="633" Height="633" Margin="0,60,0,0">

            <Grid.RowDefinitions>
                <RowDefinition Height="45*"/>
                <RowDefinition Height="45*"/>
                <RowDefinition Height="45*"/>
                <RowDefinition Height="9*"/>
                <RowDefinition Height="45*"/>
                <RowDefinition Height="9*"/>
                <RowDefinition Height="45*"/>
                <RowDefinition Height="3*"/>
                <RowDefinition Height="5*"/>
                <RowDefinition Height="45*"/>
                <RowDefinition Height="9"/>
                <RowDefinition Height="45*"/>
                <RowDefinition Height="9*"/>
                <RowDefinition Height="45*"/>
                <RowDefinition Height="9*"/>
                <RowDefinition Height="45*"/>
                <RowDefinition Height="9*"/>
                <RowDefinition Height="45*"/>
                <RowDefinition Height="27*"/>
                <RowDefinition Height="45*"/>
                <RowDefinition Height="46*"/>

            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="25*"/>
                <ColumnDefinition Width="5*"/>
                <ColumnDefinition Width="41*"/>
                <ColumnDefinition Width="11"/>
                <ColumnDefinition Width="68*"/>
                <ColumnDefinition Width="10*"/>
                <ColumnDefinition Width="192*"/>
                <ColumnDefinition Width="29*"/>
                <ColumnDefinition Width="41"/>
                <ColumnDefinition Width="11"/>
                <ColumnDefinition Width="48*"/>
                <ColumnDefinition Width="117*"/>
                <ColumnDefinition Width="11*"/>
                <ColumnDefinition Width="165*"/>
                <ColumnDefinition Width="16*"/>
                <ColumnDefinition Width="22*"/>

            </Grid.ColumnDefinitions>


            <Button x:Name="btnValorEvento" Style="{StaticResource MaterialDesignRaisedButton}" Content="VALOR EVENTO" Grid.Row="19" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height="Auto" Padding="5" FontFamily="Arial" FontSize="14" FontWeight="Bold" Grid.ColumnSpan="4" Grid.Column="2" Click="BtnValorEvento_Click" Margin="2,0,0,0"/>
            <TextBlock x:Name="txtValorEvento" Grid.Column="6" Grid.Row="19" TextWrapping="Wrap" Text="$" TextAlignment="Center" FontWeight="Bold" FontSize="14" Padding="0,11,0,0" FontFamily="Arial" />
            <TextBox x:Name="txtRutClienteContrato" materialDesign:HintAssist.Hint="Ej: 12.345.678-9" Grid.Column="6" Grid.Row="4" FontFamily="Arial" FontSize="14" />
            <TextBox x:Name="txtNumeroDeContrato" materialDesign:HintAssist.Hint="Numero de Contrato" Grid.Column="6" VerticalContentAlignment="Bottom" HorizontalContentAlignment="Stretch" Grid.Row="2" FontFamily="Arial" FontSize="14" />
            <Button x:Name="btnBuscarNumeroDeContrato" Style="{StaticResource MaterialDesignRaisedButton}" Grid.Row="2" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height="Auto" Padding="5" FontFamily="Arial" FontSize="14" FontWeight="Bold" Grid.ColumnSpan="3" Grid.Column="2" Click="BtnBuscarNumeroDeContrato_Click" >
                <StackPanel>
                    <TextBlock Text="Nº de Contrato"/>
                    <TextBlock Text="(Buscar)" FontSize="9" HorizontalAlignment="Center" Margin="20,0,0,0"/>
                </StackPanel>
            </Button>
            <Button x:Name="btnBuscarRutContrato" Style="{StaticResource MaterialDesignRaisedButton}" Grid.Row="4" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height="Auto" Padding="5" FontFamily="Arial" FontSize="14" FontWeight="Bold" Grid.ColumnSpan="3" Grid.Column="2" Click="BtnBuscarRutContrato_Click" >
                <StackPanel>
                    <TextBlock Text="RUT Cliente" FontFamily="Arial" FontSize="14"/>
                    <TextBlock Text="(Buscar)" FontSize="9" HorizontalAlignment="Center" Margin="21,0,0,0"/>
                </StackPanel>
            </Button>
            <TextBox x:Name="txtAsistentes" materialDesign:HintAssist.Hint="Ingrese Cantidad de Asistentes" Grid.Row="13" VerticalAlignment="Stretch" HorizontalContentAlignment="Right" VerticalContentAlignment="Bottom" Padding="0,0,1,0" FontFamily="Arial" FontSize="14" Grid.ColumnSpan="3" Grid.Column="4" />
            <materialDesign:PackIcon Kind="AccountMultiple" Height="Auto" Width="Auto" Grid.Row="13" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" VerticalContentAlignment="Stretch" HorizontalContentAlignment="Stretch"  Grid.Column="2" />
            <materialDesign:PackIcon Kind="AccountMultiplePlus" Height="Auto" Width="Auto" Grid.Row="15" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" VerticalContentAlignment="Stretch" HorizontalContentAlignment="Stretch"  Grid.Column="2" />
            <TextBox x:Name="txtPersonalAdicional" materialDesign:HintAssist.Hint="Ingrese Personal Adicional (*)" Grid.Row="15" VerticalAlignment="Stretch" HorizontalContentAlignment="Right" VerticalContentAlignment="Bottom" Padding="0,0,1,0" FontFamily="Arial" FontSize="14" Grid.ColumnSpan="3" Grid.Column="4"/>
            <materialDesign:PackIcon Kind="Eventbrite" Height="Auto" Width="Auto" Grid.Row="11" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" VerticalContentAlignment="Stretch" Padding="0" HorizontalContentAlignment="Stretch" Grid.Column="2" />
            <ComboBox x:Name="cboModalidadEventoContrato" materialDesign:HintAssist.Hint="Seleccione Tipo de Evento" Style="{StaticResource MaterialDesignFloatingHintComboBox}" Grid.Row="11" VerticalAlignment="Stretch" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Center" SelectedValuePath="IdModalidad" DisplayMemberPath="Nombre" FontFamily="Arial" FontSize="14" Grid.ColumnSpan="3" Grid.Column="4" SelectionChanged="CboModalidadEventoContrato_SelectionChanged" Padding="0" />

            <materialDesign:PackIcon Kind="Eventbrite" Height="Auto" Width="Auto" Grid.Row="9" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Grid.Column="2" />
            <DatePicker x:Name="fechaInicioPicker" materialDesign:HintAssist.Hint="Fecha Inicio" Grid.Column="10" Grid.Row="4" VerticalContentAlignment="Bottom" VerticalAlignment="Stretch" HorizontalContentAlignment="Stretch" FontStyle="Italic" FontFamily="Arial" FontSize="14" Padding="0" Grid.ColumnSpan="2" />
            <materialDesign:TimePicker materialDesign:HintAssist.Hint="Hora de Inicio" Is24Hours="True" x:Name="horaInicioPicker" Grid.Column="13" Grid.Row="4" VerticalAlignment="Stretch" HorizontalContentAlignment="Stretch" FontStyle="Italic"  FontFamily="Arial" FontSize="14" VerticalContentAlignment="Stretch" Margin="0" />
            <materialDesign:TimePicker materialDesign:HintAssist.Hint="Hora de Termino" Is24Hours="True" x:Name="horaTerminoPicker" Grid.Column="13" Grid.Row="6" VerticalAlignment="Stretch" HorizontalContentAlignment="Stretch" FontStyle="Italic" Margin="0" FontFamily="Arial" FontSize="14" VerticalContentAlignment="Bottom" />
            <materialDesign:PackIcon Kind="TooltipText" Height="Auto" Width="Auto" Grid.Column="8" Grid.Row="13" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
            <materialDesign:PackIcon Kind="Account" Height="Auto" Width="Auto" Grid.Row="6" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"  Grid.Column="2" />
            <TextBox x:Name="txtNombreClienteContrato" materialDesign:HintAssist.Hint="Nombre" Grid.Row="6" HorizontalContentAlignment="Right" VerticalContentAlignment="Bottom" IsEnabled="False" FontWeight="Bold"  FontFamily="Arial" FontSize="14" Grid.ColumnSpan="3" Grid.Column="4" />


            <TextBox x:Name="txtObservaciones" TextWrapping="Wrap" VerticalAlignment="Stretch" AcceptsReturn="True" VerticalScrollBarVisibility="Auto" SpellCheck.IsEnabled="True" materialDesign:HintAssist.Hint="Observaciones" Grid.Column="10" Grid.Row="13" VerticalContentAlignment="Stretch" Padding="3" FontFamily="Arial" FontSize="14" Grid.ColumnSpan="5" Grid.RowSpan="3" BorderBrush="Black" />


